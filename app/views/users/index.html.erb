<div class="header_hp center-align">
  <%=image_tag("arch-background.jpg")%>

</div>

<h4 class="center-align">Explore These Cities</h4>
<br>
<div class="divider"></div>
<br>
<div class="row">
  <% @cities.each do |city| %>
<a href='<%= get_path_from_city(city) %>'>
  <div class="col l4 m6 s12">
    <div class="card">
      <div class="real-card">
        <div class="card-image">
          <img class="city_picture container" src="<%=city.image %>">
        </div>
        <span class="card-title card_title"><%= city.name %></span>
      </div>
    </div>
  </div>
  <% end %>
</div>
<br>
<br>
